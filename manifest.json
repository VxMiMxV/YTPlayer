{
"name": "MiM's Player",
"version": "1.0",
"manifest_version": 2,
"permissions": [
"tabs",
"notifications",
"http://*/",
"https://*/",
"https://www.youtube.com/*",
"https://chrome-devtools-frontend.appspot.com/*",
"storage",
"webRequest",
"webRequestBlocking"
],

"icons": {"16": "images/app_icon_16.png","32": "images/app_icon_32.png","128": "images/app_icon_128.png"},

"description": "c.c",
"background": { "scripts": ["background.js"]},
"browser_action": { "default_icon": "images/app_icon_16.png" },

"web_accessible_resources": ["images/*.png","styles/Style1.css", "styles/Style2.css"]}